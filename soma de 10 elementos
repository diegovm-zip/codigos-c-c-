#include <stdio.h>

int main() {
int numeros[10];
int soma = 0;

// Solicita ao usuário que insira 10 números inteiros
for(int i = 0; i < 10; i++) {
    printf("Digite o %dº número inteiro: ", i + 1);
    scanf("%d", &numeros[i]);
    soma += numeros[i]; // Acumula a soma dos elementos
}

// Exibe a soma total
printf("A soma total dos elementos é: %d\n", soma);

return 0;
}
