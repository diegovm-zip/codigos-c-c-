#include <stdio.h>

#define TAMANHO_FILA 5

int main() {
    int fila[TAMANHO_FILA];
    int frente = 0, tras = 0;

    // Gerar e inserir 5 senhas na fila
    for (int i = 1; i <= TAMANHO_FILA; i++) {
        fila[tras] = i;
        tras = (tras + 1) % TAMANHO_FILA; // Atualiza a posição de "tras"
    }

    // Atendendo os clientes
    printf("Iniciando o atendimento...\n");
    while (frente != tras) {
        int senha_atendida = fila[frente];
        printf("Atendendo senha %d\n", senha_atendida);
        
        // Atualiza a frente da fila
        frente = (frente + 1) % TAMANHO_FILA;
    }

    return 0;
}
