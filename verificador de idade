#include <stdio.h>
int main() {

    int idade;


    printf("Bem-vindo ao verificador de idade!\n");

    printf("Insira sua idade: ");

    scanf("%d", &idade);



    if (idade < 18) {

        printf("Menor de idade! \n");

    } else if (idade >= 18 && idade < 60) {

        printf("O meia idade ...\n");

    } else {

        printf("olha o cemiterio!\n");

    }
   
    return 0;
}
